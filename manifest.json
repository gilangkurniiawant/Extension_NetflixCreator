{
    "manifest_version": 3,
    "name": "AKUNVIP - Netflix Creator",
    "description": "bla bla",
    "version": "1.0",
    "web_accessible_resources": [
        {
            "resources": [
                "template.html"
            ],
            "matches": [
                "https://*.netflix.com/*"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.netflix.com/*"
            ],
            "js": [
                "jquery.min.js",
                "popup.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}